{
    "name": "arbory/omnipay-paysera",
    "type": "library",
    "description": "Paysera gateway for Omnipay payment processing library",
    "keywords": [
        "paysera",
        "gateway",
        "merchant",
        "omnipay",
        "pay",
        "payment",
        "purchase"
    ],
    "homepage": "https://github.com/arbory/omnipay-paysera",
    "license": "MIT",
    "authors": [
        {
            "name": "Povilas Susinskas",
            "email": "povilassusinskas@gmail.com"
        },
        {
            "name": "Semyon Chetvertnyh",
            "email": "semyon.chetvertnyh@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Omnipay\\Paysera\\": "src/",
            "Omnipay\\Paysera\\Tests\\": "tests/"
        }
    },
    "require": {
        "ext-openssl": "*",
        "omnipay/common": "^3"
    },
    "require-dev": {
        "omnipay/tests": "^3.1",
        "phpro/grumphp": "^0.19.1",
        "squizlabs/php_codesniffer": "^3.5",
        "php-coveralls/php-coveralls": "^2.2"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
